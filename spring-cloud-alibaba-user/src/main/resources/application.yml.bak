server:
  port: 9090

spring:
  cloud:
    nacos:
      discovery:
        server-addr: localhost:8848
        namespace: test
        group: cloud
  zipkin:
    discovery-client-enabled: false #不让zipkin注册到nacos
    base-url: http://localhost:9411 # zipkin服务地址
    sender:
      type: web

  sleuth:
    sampler:
      probability: 1 # 日志记录采样率，1为100%，默认为0.1即10%

# 开启 Feign 对 Sentinel 的支持
feign:
  sentinel:
    enabled: true

